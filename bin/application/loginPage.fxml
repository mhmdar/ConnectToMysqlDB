<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Pane fx:id="pane" layoutY="-9.0" prefHeight="250.0" prefWidth="370.0">
         <children>
            <Label fx:id="usernameLabel" layoutX="77.0" layoutY="84.0" prefHeight="17.0" prefWidth="62.0" text="Usename:" />
            <Label fx:id="passwordLabel" layoutX="77.0" layoutY="115.0" prefHeight="17.0" prefWidth="62.0" text="Password:" />
            <TextField fx:id="username" layoutX="145.0" layoutY="80.0" onAction="#authenticator" promptText="username" text="admin" />
            <PasswordField fx:id="password" layoutX="145.0" layoutY="113.0" onAction="#authenticator" promptText="password" />
            <Button fx:id="loginButton" layoutX="161.0" layoutY="174.0" mnemonicParsing="false" onAction="#authenticator" onKeyPressed="#loginButtonAcceptEnter" text="Login" />
         </children>
      </Pane>
   </children>
</AnchorPane>
